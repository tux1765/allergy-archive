[*.vue]
charset = utf-8
indent_size = 2
indent_style = tab
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true

[*.{js,jsx,mjs,cjs,ts,tsx,mts,cts}]
charset = utf-8
indent_size = 4
indent_style = tab
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true
